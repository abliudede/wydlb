<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg_blur"
    android:id="@+id/rl_dialog_parent"
    android:layout_gravity="center">

    <LinearLayout
        android:id="@+id/body_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dp"
        android:background="@drawable/white_corner_card"
        android:orientation="vertical">
        
        <android.support.v7.widget.RecyclerView
            android:id="@+id/rv_feedback_types"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView android:id="@+id/tv_cancel"
            android:textColor="@color/color_black_ff333333"
            android:text="取消"
            android:background="@drawable/shape_gray_bottomyuanjiao"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="55dp" />


    </LinearLayout>


</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:padding="10dp">


    <ImageView
        android:id="@+id/iv_web_icon"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:src="@mipmap/icon_web_record"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tv_web_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/iv_web_icon"
        android:layout_toRightOf="@id/iv_web_icon"
        android:layout_marginLeft="12dp"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/text_size_15"
        android:maxLines="1"
        android:ellipsize="end"/>


    <TextView
        android:id="@+id/tv_web_url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_web_name"
        android:layout_alignBottom="@id/iv_web_icon"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/five_text_color"
        android:textSize="@dimen/small_text_size" />


</RelativeLayout>